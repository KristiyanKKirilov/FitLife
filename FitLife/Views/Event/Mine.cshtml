@model IEnumerable<EventServiceModel>

@{
	ViewData["Title"] = "My Events";
}

<h2 class="text-center" style="color:white;">@ViewBag.Title</h2>
<hr />

@if (!Model.Any())
{
	<h2 class="text-center">You have no events yet!</h2>
}

<div class="row">
	@foreach (var currentEvent in Model)
	{
		<partial name="_EventPartial" model=@currentEvent />
	}
</div>